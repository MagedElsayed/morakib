                <li class="nav-item">
                    <a href="mokhlfat.html">
                        <i class="la la-hourglass-start"></i>
                        <% if(decodedToken.role !== 'inspector'){ %>
                            <p>مخالفات قيد الإنتظار</p>
                            <span class="badge badge-count badge-warning"><%= pendingPenCount %></span>
                        <% } %>
    
                    </a>
                </li>
