                                <a href="#info">
                                    <span class="link-collapse">البيانات الشخصية</span>
                                </a>
                            </li>
                            <li>
                                <a href="#logout">
                                    <span class="link-collapse">تسجيل خروج</span>
                                </a>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
            <ul class="nav">
                <li class="nav-item active">
                    <% if(decodedToken.role === 'admin'){ %>
                        <a href="/admin">
                    <% } else if(decodedTokem === 'manager'){ %>
                        <a href="/manager">
                    <% } else if(decodedTokem === 'inspector'){ %>
                        <a href="/inspector">
                    <% } %>
                        <i class="la la-home"></i>
                        <p>الرئيسية</p>
                    </a>
                </li>
                <li class="nav-item">
                    <a href="/gawlat">
